{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "strict": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "outDir": "lib",
    "esModuleInterop": true,
    "moduleResolution": "Node16",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
