{
  "compilerOptions": {
    "target": "ES2023",
    "module": "nodenext",
    "lib": ["ES2023"],
    "strict": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "outDir": "lib",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "moduleResolution": "nodenext",
    "moduleDetection": "force",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
