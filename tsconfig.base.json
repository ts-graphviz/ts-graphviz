{
  "compilerOptions": {
    "target": "ES2022",
    "module": "Node16",
    "lib": ["ES2022"],
    "strict": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "outDir": "lib",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "moduleResolution": "Node16",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
