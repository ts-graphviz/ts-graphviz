{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "strict": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "outDir": "lib",
    "esModuleInterop": true,
    "moduleResolution": "Node16",
    "skipLibCheck": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "#test/*": ["./test/*/index.js"],
      "ts-graphviz": ["./src/index.js"],
      "ts-graphviz/ast": ["./src/ast/index.js"]
    }
  }
}
