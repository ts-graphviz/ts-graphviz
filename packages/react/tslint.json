{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "no-console": false,
    "jsx-wrap-multiline": false,
    "prettier": true,
    "import-spacing": true,
    "array-type": false,
    "quotemark": [
      true,
      "single"
    ],
    "arrow-parens": false,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ]
}
