{
  "name": "@ts-graphviz/ast",
  "version": "2.0.1",
  "description": "Graphviz AST(Abstract Syntax Tree) Utilities",
  "keywords": [],
  "homepage": "https://github.com/ts-graphviz/ts-graphviz#readme",
