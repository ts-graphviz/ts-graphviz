{
  "name": "@ts-graphviz/adapter",
  "version": "2.0.0",
  "description": "",
  "type": "module",
  "main": "./lib/node.cjs",
  "module": "./lib/node.js",
  "types": "./lib/node.d.ts",
  "exports": {
    ".": {
      "browser": {
        "require": "./lib/browser.cjs",
        "import": "./lib/browser.js",
        "types": "./lib/browser.d.ts"
      },
      "deno": {
        "types": "./lib/deno/mod.d.ts",
        "default": "./lib/deno/mod.js"
      },
      "node": {
        "require": "./lib/node.cjs",
        "import": "./lib/node.js",
        "types": "./lib/node.d.ts"
      },
      "default": {
        "types": "./lib/node.d.ts",
        "require": "./lib/node.cjs",
        "import": "./lib/node.js"
      }
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ts-graphviz/ts-graphviz.git",
    "directory": "packages/adapter"
  },
  "author": "Yuki Yamazaki <yuki@kamiazya.tech>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ts-graphviz/ts-graphviz/issues"
  },
  "homepage": "https://github.com/ts-graphviz/ts-graphviz#readme",
  "dependencies": {
    "@ts-graphviz/common": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.2.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.8",
    "vite-plugin-dts": "^2.3.0"
  },
  "scripts": {
    "build": "vite build && rm ./lib/deno.cjs ",
    "postbuild": "prettier --write ./lib/**/*.{js,cjs,d.ts}"
  }
}
