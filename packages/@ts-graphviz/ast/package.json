  "dependencies": {
    "@ts-graphviz/common": "^2.0.0"
  },
  "devDependencies": {
    "peggy": "^2.0.1",
    "ts-pegjs": "^3.1.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.8",
    "vite-plugin-dts": "^2.3.0"
  },
  "scripts": {
    "prebuild": "peggy --plugin ts-pegjs --extra-options-file src/dot-shim/parser/peggy.options.json -o src/dot-shim/parser/_parse.ts src/dot-shim/parser/dot.peggy",
